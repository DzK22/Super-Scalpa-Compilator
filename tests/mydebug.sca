program debug
var tab : array [1..3, 1..3] of int;
var i, j : int;
function power (x : int, y : int) : int
begin
    if y = 0 then
        return 1
    else if y mod 2 = 0 then
        return power(x, y/2) * power(x, y/2)
    else
        return x * power(x, y/2) * power(x, y/2);
end;
begin
    write power(2,0);
    write "\n";
    i := 3;
    j := 3;
    write tab[i,j];
    write "\n";
    tab[3,3] := 45;
    write tab[3,3];
    write "\n";
end
