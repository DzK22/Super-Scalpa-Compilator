program pgcd
    var a, b, c : int;
    begin
        read a;
        read b;
        c := a;
        while a <> b do
            begin
                if a > b then a:= a - b
                         else b := b - a;
            end;
        write "le pgcd de (";
        write c;
        write " et ";
        write b;
        write ") est: ";
        write a;
        write "\n";
    end
