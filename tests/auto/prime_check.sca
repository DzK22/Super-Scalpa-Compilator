program prime_check
    var n, r, i : int;
    begin
        n := 7 ;
        r := 0;
        i := 1;
        while i <= n do
        begin
            if (n MOD i) = 0 then r := r + 1;
            i := i + 1;
        end;
        write n;
        if r = 2 then
            write " est premier\n"
        else
            write " n'est pas premier\n";
    end
