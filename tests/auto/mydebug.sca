program mydebug
    var tab : array [1..10, 1..10,1..10] of bool;
begin
    tab[1,1,1] := true;
    tab[2,2,2] := false;
    tab[3,3,3] := true;
    write tab[1,1,1] ;
    write "\n";
    write tab[2,2,2];
    write "\n";
    write tab[3,3,3];
    write "\n";
    write tab[1,1,1] or tab[2,2,2] and tab[3,3,3];
    write "\n";
end
