(* Test des opérations booléènnes  *)

program boolTest
   var a, b, c ,d: bool
   begin
     a := false;
     b := true;

     c := not a ;
     write c ;
     write "\n" ;

     c := a and b ;
     write c ;
     write "\n" ;

     c := a or b ;
     write c ;
     write "\n" ;

     c := not ( a xor b ) ;
     write c ;
     write "\n" ;

     c := not c or a and not a ; (* priority *)
     write c ;
     write "\n" ;


   end
