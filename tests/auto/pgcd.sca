program pgcd
    var a, b, c, d : int;
    begin
        a := 15;
        b := 6;
        c := a;
        d := b;
        while a <> b do
            begin
                if a > b then a:= a - b
                         else b := b - a;
            end;
        write "le pgcd de (";
        write c;
        write " et ";
        write d;
        write ") est: ";
        write a;
        write "\n";
    end
