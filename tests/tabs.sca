program tabz
    var tab : array[1..3,1..3] of int ;

    function toto (tata : array[1..3,1..3] of int) : unit
    begin
        tata[1,3] := 3;
        write tata[1,2] * tata[1,3];
        write "\n";
    end;
    begin
        tab[1,2] := 1;
        tab[1,3] := 2;
        toto(tab);

        write tab[1,2] * tab[1,3];
        write "\n";
     end
