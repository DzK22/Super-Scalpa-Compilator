program fibo
    var d : int;
    function fib (n : int) : int
    begin
        if (n = 0) then return 0;
        if (n = 1 or n = 2) then return 1;

        (* n >= 2 *)
        return fib(n - 1) + fib(n - 2);
    end;

    begin
        d := 4;
        write "fib(";
        write d;
        write ") = ";
        write fib(4);
        write "\n";
    end
